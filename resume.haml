- header = File.read('resumeheader.haml')
!!! 5
%html
  %head
    %link{:href=>"http://fonts.googleapis.com/css?family=Julius+Sans+One", :rel=>"stylesheet", :type=>"text/css"}
    %link{:href=>"http://fonts.googleapis.com/css?family=Source+Sans+Pro:200,300,400,600,700", :rel=>"stylesheet", :type=>"text/css"}
    %link{:href=>"bootstrap/css/bootstrap.min.css", :rel=>"stylesheet"}
    %link{:href=>"style.css", :rel=>"stylesheet"}
  %body
    = Haml::Engine.new(header).render
    .vspacer80

    .body-wide{:style=>"background-color:transparent;"}
      .ninehundred{:style=>"text-align:center;"}
        %img{:src=>"resume.png", :style=>"margin:-20px;position:relative;left:-35px;"}

    .vspacer80

    %footer
    %script{:src=>"jquery-1.8.2.min.js"}
    %script{:src=>"scrolltop.js"}
    %script{:src=>"bootstrap/js/bootstrap.min.js"}
    :javascript
      $("div[class^=column] .img").hover(function() {
        $(this).children(".blackout").hide();
      },function() {
        $(this).children(".blackout").show();
      });




